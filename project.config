[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/hhalex
[access "refs/for/refs/*"]
	read = group user/hhalex
	push = group user/hhalex
	label-Code-Review = -2..+2 group user/hhalex
	label-Verified = -1..+1 group user/hhalex
	rebase = group user/hhalex
	removeReviewer = group user/hhalex
	submit = group user/hhalex
[access "refs/heads/*"]
	read = group user/hhalex
	create = group user/hhalex
	push = +force group user/hhalex
	pushMerge = group user/hhalex
[access "refs/tags/*"]
	push = +force group user/hhalex
	pushTag = group user/hhalex
	pushSignedTag = group user/hhalex
